\newcommand*{\DRDC@convertfile}[2]{%
  \IfFileExists{#1}{%
    \begingroup
    \newread\DRDC@infile
    \newwrite\DRDC@outfile
    \immediate\openin\DRDC@infile #1%
    \immediate\openout\DRDC@outfile #2%
    \typeout{Writing to #2}%
    \makeatletter
    \loop
      \immediate\read\DRDC@infile to\DRDC@line
      \DRDC@stripendspace\DRDC@line
      \immediate\write\DRDC@outfile{\DRDC@line}%
      \ifeof\DRDC@infile\else
    \repeat
    \immediate\closein\DRDC@infile
    \immediate\closeout\DRDC@outfile
    \endgroup
  }{\PackageError{drdc-translate}{Cannot open #1 or file is empty}{}}%
}
