  \setlength\DRDC@colone{0.32\textwidth}
  \setlength\DRDC@coltwo{0.16\textwidth}
  \ifDRDC@isfrench
    \setlength\DRDC@colthree{0.045\textwidth}
    \setlength\DRDC@colfour{0.115\textwidth}
    \setlength\DRDC@colfive{0.32\textwidth}
  \else
    \setlength\DRDC@colthree{0.05\textwidth}
    \setlength\DRDC@colfour{0.20\textwidth}
    \setlength\DRDC@colfive{0.23\textwidth}
  \fi
  \setlength\DRDC@colonetwo{\DRDC@colone}
  \addtolength\DRDC@colonetwo{\DRDC@coltwo}
  \setlength\DRDC@colonetothree{\DRDC@colonetwo}
  \addtolength\DRDC@colonetothree{\DRDC@colthree}
  \setlength\DRDC@colthreefour{\DRDC@colthree}
  \addtolength\DRDC@colthreefour{\DRDC@colfour}
  \setlength\DRDC@coltwotofour{\DRDC@coltwo}
  \addtolength\DRDC@coltwotofour{\DRDC@colthree}
  \addtolength\DRDC@coltwotofour{\DRDC@colfour}
  \setlength\DRDC@colfourfive{\DRDC@colfour}
  \addtolength\DRDC@colfourfive{\DRDC@colfive}
  \setlength\DRDC@colthreetofive{\DRDC@colfourfive}
  \addtolength\DRDC@colthreetofive{\DRDC@colthree}
  \setlength\DRDC@colall{\DRDC@colonetothree}
  \addtolength\DRDC@colall{\DRDC@colfourfive}
  \ifDRDC@isbrief\else
    \ifDRDC@isfrench
      \renewenvironment{abstract}{\DRDC@gobbleabstract}{}
    \else
      \renewenvironment{fabstract}{\DRDC@gobbleabstract}{}
    \fi
